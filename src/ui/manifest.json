{
  "manifest_version": 3,
  "name": "Tiny Website Blocker Chrome Extension",
  "description": "This scripts blocks websites.",
  "version": "0.1",
  "options_page": "options.html",
  "permissions": ["storage", "tabs", "activeTab"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": { "service_worker": "background.js", "type": "module" },
  "minimum_chrome_version": "92"
}
